using CLOUDS_PRODUCTS_STOCKS as CLOUDS_PRODUCTS_STOCKS;
using CLOUDS_PRODUCTS_CATALOGSERVICE_STOCKAVAILABILITIES as CLOUDS_PRODUCTS_CATALOGSERVICE_STOCKAVAILABILITIES;
view CLOUDS_PRODUCTS_CATALOGSERVICE_STOCKS as select from CLOUDS_PRODUCTS_STOCKS as STOCKS mixin {
  __AVAILABILITY : association to CLOUDS_PRODUCTS_CATALOGSERVICE_STOCKAVAILABILITIES on __AVAILABILITY.CODE = $projection.AVAILABILITY_CODE;
} into {
  STOCKS.MODIFIEDAT,
  STOCKS.CREATEDAT,
  STOCKS.CREATEDBY,
  STOCKS.MODIFIEDBY,
  STOCKS.ID,
  STOCKS.QUANTITY,
  STOCKS.MINIMUMQUANTITY,
  __AVAILABILITY as AVAILABILITY,
  STOCKS.AVAILABILITY_CODE
};
