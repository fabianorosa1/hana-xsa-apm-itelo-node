using CLOUDS_ITELO_REVIEWS as CLOUDS_ITELO_REVIEWS;
using CLOUDS_PRODUCTS_CATALOGSERVICE_PRODUCTS as CLOUDS_PRODUCTS_CATALOGSERVICE_PRODUCTS;
using CLOUDS_PRODUCTS_CATALOGSERVICE_REVIEWERS as CLOUDS_PRODUCTS_CATALOGSERVICE_REVIEWERS;
view CLOUDS_PRODUCTS_CATALOGSERVICE_REVIEWS as select from CLOUDS_ITELO_REVIEWS as REVIEWS mixin {
  __PRODUCT : association to CLOUDS_PRODUCTS_CATALOGSERVICE_PRODUCTS on __PRODUCT.ID = $projection.PRODUCT_ID;
  __REVIEWER : association to CLOUDS_PRODUCTS_CATALOGSERVICE_REVIEWERS on __REVIEWER.ID = $projection.REVIEWER_ID;
} into {
  REVIEWS.MODIFIEDAT,
  REVIEWS.CREATEDAT,
  REVIEWS.CREATEDBY,
  REVIEWS.MODIFIEDBY,
  REVIEWS.ID,
  __PRODUCT as PRODUCT,
  __REVIEWER as REVIEWER,
  REVIEWS.TITLE,
  REVIEWS.MESSAGE,
  REVIEWS.RATING,
  REVIEWS.HELPFULCOUNT,
  REVIEWS.HELPFULTOTAL,
  REVIEWS.PRODUCT_ID,
  REVIEWS.REVIEWER_ID
};
